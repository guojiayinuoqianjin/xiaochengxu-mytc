<!-- Text -->
<template name="item_text">
  <view name="{{index}}" class="template_item_view flex template_flex_box" style="justify-content: flex-start;align-items: center">
    <text class="f-15rem">{{content.value}}</text>
  </view>
</template>
<!--内容-->
<template name="item_content">
  <view class="item_content">
    <textarea name="{{index}}" class="input" placeholder="{{content.placeholder}}" value="{{content.value}}" bindinput="item_content_save" data-row="{{index}}" />
  </view>
</template>
<!--标题 + 内容-->
<template name="item_title_content">
  <view class="item_title_content">
    <view class="title">{{content.title}}</view>
    <input name="{{index}}" class="input" type="{{content.inputType ? content.inputType : 'text'}}" placeholder="{{content.placeholder}}" value="{{content.value}}" disabled="{{!content.canTap}}" maxlength="{{content.maxlength}}" bindinput="item_title_content_save"
      data-row="{{index}}" />
  </view>
</template>
<!--内容+按钮-->
<template name="item_content_button">
  <view class="item_content_button">
    <button class="{{content.changebak}}" style="font-size:30rpx;margin-left:5px;margin-right:0px;height:88rpx;line-height:88rpx;width:115px;color:white;float:right;" catchtap="getCode">{{content.changecontent}}</button>
    <input name="{{index}}" class="input" type="{{content.inputType ? content.inputType : 'text'}}" placeholder="{{content.placeholder}}" disabled="{{!content.canTap}}" maxlength="{{content.maxlength}}" bindinput="item_ontent_botton_save" data-row="{{index}}"
    />

  </view>
</template>
<!--标题 + 下拉列表-->
<template name="item_title_picker">
  <view class="item_title_picker">
    <view class="title">{{content.title}}</view>
    <picker class="pickerwidth" bindchange="bindPickerChange" value="{{content.listitem[0]}}" range="{{content.listitem}}" data-id="{{index}}">
      <view class="picker">
        {{content.value}}
      </view>
    </picker>
  </view>
</template>

<!--标题 + 星号 + 内容-->
<template name="item_title_star_content">
  <view name="{{index}}" class="item_title_star_content">
    <view>
      <text class="title">{{content.title}}
        <text class="star">*</text>
      </text>

    </view>
    <input name="{{index}}" class="input" type="{{content.inputType ? content.inputType : 'text'}}" placeholder="{{content.placeholder}}" value="{{content.value}}" disabled="{{!content.canTap}}" bindinput="item_title_star_content_save" data-row="{{index}}"
    />
  </view>
</template>
<template name="item_title_record">
  <view class="recordContentView">
    <image src="" class="recordImageView"></image>
  </view>
</template>
<!--12-16      ------------------------------------------------------------------------------->

<!--分割线 横向 高度 1rpx-->
<template name="item_separate_horizontal">
  <view class="defaultBackgroundColor" style="height: 1px;margin: {{content.lineMargin}};"></view>
</template>

<!--分割线 横向 宽度 3px-->
<template name="item_padding">
  <view class="defaultBackgroundColor" style="height: 20rpx;margin: {{content.lineMargin}};"></view>

  <!--<view class="lineColor" style="height: 1px;margin: {{content.lineMargin}};"></view>-->
</template>

<!--分割线 纵向 宽度 1px-->
<template name="item_separate_vertical">
  <view class="lineColor" style="width: 1.3px;height: {{content.lineHeight}}rpx;display: inline"></view>
</template>

<!--标题1&输入框 + 标题2&输入框-->
<template name="item_price_origin_discount">
  <view class="template_item_view template_flex_box flex">
    <view class="item_price_origin_discount">
      <text class="f-15rem">{{content.title1}}</text>
      <input name="{{index}}-first" class="f-15rem" type="number" placeholder="{{content.placeholder1}}" bindinput="item_price_origin_discount_first_save" data-row="{{index}}" value="{{content.value1}}" />
    </view>
    <template is="item_separate_vertical" data="{{content}}"></template>
<view class="item_price_origin_discount" style="margin-left: 20rpx">
  <text class="f-15rem">{{content.title2}}</text>
  <input disabled="false" name="{{index}}-second" style="text-align: right;color:darkgrey;" class="f-15rem" type="number" placeholder-style="color:red;" placeholder="{{content.placeholder2}}" bindinput="item_price_origin_discount_second_save" data-row="{{index}}"
    value="{{content.value2}}" />
</view>
</view>
</template>

<!-- 15、	服务时间选择，周排 和 月排             选择按钮-->
<template name="item_serviceTime">
  <view class="template_item_view template_flex_box flex">
    <text class="item_serviceTime_title f-14rem">{{content.title}}</text>
    <view class="item_serviceTime_red_radius">
      <view class="item_serviceTime_child infoTextColor f-12rem {{content.weekSS}}" bindtap="showWeek" data-id="{{index}}">
        {{content.week}}
      </view>
      <view class="item_serviceTime_child infoTextColor f-12rem {{content.monthSS}}" bindtap="showMonth" data-id="{{index}}">
        {{content.month}}
      </view>
    </view>
  </view>
  <view class="flex item_serviceTime_detail">
    <block wx:for="{{content.date}}" wx:for-index="position" wx:key='unique'>
      <text class="item_serviceTime_detail_text contentTextColor f-12rem {{item.select?'item_serviceTime_detail_text_select':''}}" catchtap="onTimeItemClick" data-row="{{index}}" data-id="{{position}}">{{item.content}}
      </text>
    </block>
  </view>
</template>


<!-- 16、	服务时间选择，周排 和 月排               列表-->
<template name="item_serviceTime_detail">
  <view class="flex item_serviceTime_detail">
    <block wx:for="{{content.date}}" wx:key='unique'>
      <text class="item_serviceTime_detail_text contentTextColor f-14rem">{{item}}</text>
    </block>
  </view>
</template>

