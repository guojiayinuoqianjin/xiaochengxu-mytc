          {{content.startTime==''?content.placeholder1:content.startTime}}
        </view>
      </picker>
      <text class="f-15rem contentTextColor" style="margin: 20rpx; ">至</text>
      <picker mode="date" name="endTime{{index}}" value="{{content.endTime}}" start="2017-01-01" end="2020-09-01" bindchange="endTimePick" data-id="{{index}}">
        <view style="text-align: right;color:{{content.endTime==''?'lightgray':'black'}};" class="f-15rem">
          {{content.endTime==''?content.placeholder2:content.endTime}}
        </view>
      </picker>
    </view>
  </view>
</template>

