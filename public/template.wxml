      <text class="f-15rem">{{content.title}}</text>
    </view>
    <view class="item_title_date_content">
      <picker mode="date" name="startTime{{index}}" value="{{content.startTime}}" start="2017-01-01" end="2020-09-01" bindchange="startTimePick" data-id="{{index}}">
