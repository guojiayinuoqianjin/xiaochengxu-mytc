  <view class="huifu" wx:if="{{item.SubCommentList.length<10 && item.CommentCount>10}}" data-index="{{idx}}" catchtap="GetMoreSubComment">查看所有{{item.CommentCount}}评论</view>
</view>
</view>
</view>
</block>

