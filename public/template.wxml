<wxs module="md">
  /*qq表情包*/
  function getFacePage(arg){ 
    var qqface = arg;
    var len = qqface.length,
      facePageArr = [],
      facePageNum = ~~(len / 27);
    for(var i = 0;i < facePageNum ; i++){ 
      facePageArr.push([])
      for(var k = 26;k>=0;k--){
        facePageArr[i].push(qqface[27*i + k])
      } 
      facePageArr[i].reverse()
    } 
    facePageArr.push(qqface.slice(27*facePageNum))
      return facePageArr 
    }
    /*二手*/
    function judgeSecond(arg){
      return ["286121","295257","295258"].indexOf(arg)
    }
    /*学历*/
    function judgeAcademic(arg){
      var academic = {
        "1":"高中以下",
        "2":"高中",
        "3":"中专/技校",
        "4":"大专",
        "5":"本科",
        "6":"硕士",
        "7":"博士",
        "8":"MBA/EMBA"
      }
      return academic[arg]
    }
    /*年纪*/
    function judgeAge(arg){
      var age = {   
        "0":"不限",
        "1":"25岁以下",
        "2":"30岁以下",
        "3":"35岁以下",
        "4":"40岁以下",
        "5":"50岁以下",
