.navigatorItem{
    display:block;
    width: 64px;
    margin-bottom: 16rpx
}
.navigatorItem::before{
    content: '';
    display: block;
    margin: 0 auto 2px;
    width: 21px;
    height: 21px;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 117px
}

.navigatorItem-service::before{
    background-position:-70px -25px
}
.navigatorItem-order::before{
    background-position:-93px -2px
}
.navigatorItem-myPublish::before{
    background-position:-25px -25px
}
.navigatorItem-publish::before{
    background-position:-2px -94px
}
.navigatorItem-greetingCard::before{
    background-position: -24px -71px
}
.navigatorItem-settled::before{
    background-position: -70px -48px
}

.navigatorItem-personalCenter::before{
    background-position:-48px -2px
}
.navigatorItem-index::before{
    background-position:-48px -71px
}
.navigatorItem-localNews::before{
    background-position:-2px -2px
}
.navigatorItem-store::before{
    background-position:-2px -25px
}
.navigatorItem-poster::before{
    background-position:-48px -48px
}
.navigatorItem-moreGoods::before{
    background-position:-2px -48px
}
.navigatorItem-activity::before{
    background-position:-48px -25px
}
.navigatorItem-link::before{
    background-position:-25px -94px
}
.navigatorItem-earn::before{
    background-position:-94px -25px
}
.navigatorItem-report::before{
    background-position:-71px -2px
}

/* 转发 */
.navigatorItem-repost{
    margin:0;
    width: 64px;
    font-size: 12px;
    line-height: 1.35
}
.navigatorItem-repost::before{
    display: block;
    content: '';
    margin: 0 auto 2px;
    width: 21px;
    height: 21px;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100%
}

