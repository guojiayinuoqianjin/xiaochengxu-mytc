 .icon_index_gonggao,
.icon_shangjia_ruzhu,
.icon_local,
.icon_dongtai_watch,
.icon_dongtai_pl,
.icon_yuyue,
.shangjia_bottom_nav icon,
.p_replay .upload_pic_ul01 .li icon,
.pr_bot .emjio,
.pr_bot .pic,
.p_icon01,
.icon_clock,
.icon_jian,
.icon_jia,
.ymyh_cell .bg01,
.ymyh_cell .bg02,
.qiang_yh_title .infor .number,
.icon_shandian,
.icon_time,
.icon_yh_call,
.icon_youhuiquan,
.icon_more_youhui,
.icon_jinru_shop,
.ruzhu_cell .icon_size {
  background-image: url(https://i.vzan.cc/citysource/xcx_heka/xcx_small_icon.png);
  background-size: 500px;
  background-repeat: no-repeat;
} 
.icon_index_gonggao {
  width: 23px;
  height: 20px;
  background-position: 0 0;
  display: block;
  margin-right: 15px;
}
.icon_shangjia_ruzhu {
  width: 22px;
  height: 22px;
  background-position: -25px 0;
  margin: 0 auto 3px;
  display: block;
}
.icon_local {
  width: 16px;
  height: 18px;
  background-position: -67px 0;
  margin-right: 4px;
  display: inline-block;
  vertical-align: -3px;
}
.icon_dongtai_watch,
.icon_dongtai_pl {
  width: 15px;
  height: 12px;
  vertical-align: -1px;
  margin: 0 3px 0 17px;
}
.icon_dongtai_watch {
  background-position: 0 -24px;
}
.icon_dongtai_pl {
  background-position: -17px -24px;
}
.icon_yuyue {
  width: 34px;
  height: 34px;
  background-position: 0 -38px;
  display: block;
  position: absolute;
  top: 0;
  left: 0;
}
.icon_zaixianmaidan {
  background-position: -166px -16px;
}
.icon_daohangdaojia {
  background-position: -195px -16px;
}
.icon_lianxishangjia {
  background-position: -224px -16px;
}
.p_replay .upload_pic_ul01 .li icon {
