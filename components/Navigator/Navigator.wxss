@import '../component.wxss';
.navigator-wrapper{
    position: fixed;
    bottom: 135px;
    right:0;
    min-width:48px;
    min-height: 42px;
    z-index:1000
}
.navigator-container{
    position: absolute;
    top: 0;
    left:48px;
    transition:transform .28s cubic-bezier(0.000, 0.890, 0.640, 0.875);
}
.showNavigator{
    transform: translateX(calc(-100%))
}
.navigator-btn{
    position: absolute;
    top: 0;
    bottom: 0;
    margin:auto 0;
    left:-48px;
    width:48px;
    height:42px;
    color: #fff;
    font-size: 12px;
    text-align: center;
    border-top-left-radius: 6rpx;
    border-bottom-left-radius: 6rpx;
    background: rgba(0, 0, 0, 0.48)
}

.icon-arrow{
    width:14px;
    height:14px;
    margin: 0 4rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100%;
}
.rotateArrow{
    transform: rotate(180deg)
}
.navigator-btn .tag{
    width:24px
}
.navigator-content{
    width: 192px;
    font-size: 12px;
    color: #333;
    padding-top: 16rpx;
    text-align: center;
    background: #fff
}