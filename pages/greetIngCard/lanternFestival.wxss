    top: 0;
    left: 0;
    animation: movearound01 15s infinite;
    -webkit-animation: movearound01 15s infinite;
}

@keyframes  movearound01{
    0%{left: 0;top: 10px}
    25%{left: 5rem;}
    50%{left: 0;top:0;}
    75%{left:  5rem;}
    100%{left: 0;top: 10px}
}

.lantern-pic03{
