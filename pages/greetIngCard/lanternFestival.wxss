.rel{position: relative;}

.none{display: none;}

.lantern-wrapper{
    position: relative;
    width: 100%;
    min-height: 100vh;
    font-size: 28rpx;
    line-height: 1;
    text-align: center;
    background: #1f224f;
    -webkit-text-size-adjust: 100%;
    -ms-text-size-adjust: 100%;
    -webkit-font-smoothing: antialiase;
    overflow: hidden;
}

.lantern-content{
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    z-index: 3
}

.star-shine{
    position: absolute;
    top: 0;
    left: 0;
    height: 100%;
    width: 100%;
    background: url(data:image/png;base64,) repeat-y 0 0;
    background-size: 100%;
    -webkit-animation: shine03 4s ease-in-out infinite;
    animation: shine03 4s ease-in-out infinite;
    opacity: 0;
    z-index: 1
}

@keyframes shine03{ 
    0% {
        opacity: 0;
    }

    25% {
        opacity: .5;
    }

    50% {
        opacity: 1;
    }

    75% {
        opacity: .5;
    }

    100% {
        opacity: 0;
    } 
} 

