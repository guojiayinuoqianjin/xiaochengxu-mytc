@keyframes rotato{
    0% {transform: rotate(0);}
    100% {transform: rotate(360deg);}
}    

.swing01{
    -webkit-animation: swing01 3s infinite;
    animation: swing01 3s infinite
}

.swing02{
    -webkit-animation: swing02 3s infinite;
    animation: swing02 3s infinite
}

@-webkit-keyframes swing01{
    0%{-webkit-transform: rotate(10deg);-webkit-transform-origin: 50% 0;}
    25%{-webkit-transform: rotate(-10deg);-webkit-transform-origin: 50% 0;}
    50%{-webkit-transform: rotate(10deg);-webkit-transform-origin: 50% 0;}
    75%{-webkit-transform: rotate(-10deg);-webkit-transform-origin: 50% 0;}
    100%{-webkit-transform: rotate(10deg);-webkit-transform-origin: 50% 0;}
}
@keyframes swing01{
    0%{transform: rotate(-10deg);transform-origin: 50% 100%;}
    25%{transform: rotate(10deg);transform-origin: 50% 100%;}
    50%{transform: rotate(-10deg);transform-origin: 50% 100%;}
    75%{transform: rotate(10deg);transform-origin: 50% 100%;}
    100%{transform: rotate(-10deg);transform-origin: 50% 100%;}
}

@-webkit-keyframes swing02{
    0%{-webkit-transform: rotate(-10deg);-webkit-transform-origin: 50% 0;}
    25%{-webkit-transform: rotate(10deg);-webkit-transform-origin: 50% 0;}
    50%{-webkit-transform: rotate(-10deg);-webkit-transform-origin: 50% 0;}
    75%{-webkit-transform: rotate(10deg);-webkit-transform-origin: 50% 0;}
    100%{-webkit-transform: rotate(-10deg);-webkit-transform-origin: 50% 0;}
}

@keyframes swing02{
    0%{transform: rotate(10deg);transform-origin: 50% 100%;}
    25%{transform: rotate(-10deg);transform-origin: 50% 100%;}
    50%{transform: rotate(10deg);transform-origin: 50% 100%;}
    75%{transform: rotate(-10deg);transform-origin: 50% 100%;}
    100%{transform: rotate(10deg);transform-origin: 50% 100%;}
}


