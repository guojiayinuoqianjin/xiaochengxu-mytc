    position: absolute;
    bottom: 160rpx;
}

.close {
    position: absolute;
    display: inline-block;
    width: 60rpx;
    height: 60rpx;
    right: 20rpx;
    top: 20rpx;
    overflow: hidden;
}

.close::before {
    content: '';
    position: absolute;
    height: 2px;
    width: 100%;
    top: 50%;
    left: 0;
    margin-top: -1px;
    background: #ccc;
    border-radius: 120% 0;
}

.close::after {
    content: '';
    position: absolute;
    height: 2px;
    width: 100%;
    top: 50%;
    left: 0;
    margin-top: -1px;
    background: #ccc;
    border-radius: 120% 0;
}

.close::before {
    transform: rotate(45deg);
}

.close::after {
    transform: rotate(-45deg);
}
