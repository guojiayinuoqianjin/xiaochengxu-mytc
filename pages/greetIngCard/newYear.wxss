    content: '';
    position: absolute;
    height: 2px;
    width: 100%;
    top: 50%;
    left: 0;
    margin-top: -1px;
    background: #ccc;
    border-radius: 120% 0;
}

.close::after {
    content: '';
    position: absolute;
    height: 2px;
    width: 100%;
    top: 50%;
    left: 0;
    margin-top: -1px;
    background: #ccc;
    border-radius: 120% 0;
}

.close::before {
    transform: rotate(45deg);
}

.close::after {
