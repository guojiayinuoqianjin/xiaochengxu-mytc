@import "../../css/cell.wxss";
view {
    color: #333;
    box-sizing: border-box;
}

.store-name {
    font-size: 32rpx;
    margin-left: 20rpx;
}

.pay-main {
    font-size: 32rpx;
    width: 690rpx;
    height: 120rpx;
    background: rgba(250, 250, 250, 1);
    border: 1px solid rgba(204, 204, 204, 1);
    margin: 0 auto;
    padding: 0 20rpx;
}

.pay-main-tips {
    font-weight: 700;
}
.basewrap {
    min-width: 320px;
    max-width: 640px;
    margin: 0 auto;
}
.online-pay-container {
    background: #fff;
    height:100%;
}.fix-bl {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    z-index: 4;
}
.top-line:before {
    content: " ";
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 1px;
    border-bottom: 1px solid #e5e5e5;
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    -webkit-transform: scaleY(0.5);
    transform: scaleY(0.5);
}
.online-pay-top {
    line-height: 110rpx;
    padding: 0 30rpx;
}

.pay-input {
    text-align: right;
}

.online-pay-top {}

.icon-city-store {
    font-size: 36rpx;
}

.coupon-label {
    width: 32rpx;
    height: 32rpx;
    background: rgba(255, 113, 5, 1);
    color: #fff;
    font-size: 24rpx;
    text-align: center;
    line-height: 32rpx;
}

.coupon-label-text {
    font-size: 28rpx;
    margin-left: 10rpx;
}

.nothing-use {
    font-size: 28rpx;
    color: #999999;
}

.coupon-wrp {
    padding:40rpx 30rpx 0;
}

.pay-amount-text {
    font-size:  32rpx;
    font-weight: 700;
}

.pay-money {
    color: #F90606;
    font-size: 24rpx;
    font-weight: 700;
}

.using-tips {
    font-size: 28rpx;
    margin: 0 auto;
    text-align: center;
    line-height: 86rpx;
    width:690rpx;
    border-top:1px solid #CCCCCC;
}   

.coupon-item {
    margin: 0 auto 20rpx;
    width: 90%;    
    box-shadow:0px 0px 38rpx 0px rgba(0, 0, 0, 0.06);
}


.coupon-list-left {
    width: 180rpx;
    height: 166rpx;
    background: linear-gradient(90deg, rgba(255, 84, 0, 1), rgba(250, 15, 103, 1));
    color: #fff;
    position: relative;
    float: left;
    border-radius: 10rpx 0px 0px 10rpx;
}

.money-label {
    font-weight: 700;
    font-size: 28rpx;
    color: #fff;
}

.money-amount {
    font-weight: 700;
    font-size: 50rpx;
    color: #fff;
}

.coupon-condition {
    font-size: 24rpx;
    padding-top: 20rpx;
    color: #fff;
}

.coupon-list-right {
    background: #fff;
    height: 166rpx;
    width: 510rpx;
    padding-top: 20rpx;
    padding-left: 20rpx;
    float: left;
    border-radius: 0px 10rpx 10rpx 0px;
    overflow: hidden;
    position: relative;
}

.only-tip {
    width: 100rpx;
    line-height: 28rpx;
    background: linear-gradient(90deg, rgba(238, 218, 163, 1), rgba(214, 189, 120, 1));
    border-radius: 14rpx;
    text-align: center;
    font-size: 20rpx;
    margin: 0 20rpx 0 0;
}

.coupon-name {
    font-size: 24rpx;
    font-weight: 700;
    width: 260rpx;
    line-height: 1.3;
}

.coupon-tips {
    color: #999;
    font-size: 24rpx;
    padding: 10rpx 0;
}

.use-condition {
    font-size: 24rpx;
    color: red;
    padding-top: 24rpx;
}

.fresh-man {
    width: 200rpx;
    line-height: 36rpx;
    background: linear-gradient(90deg, rgba(255, 84, 0, 1), rgba(250, 15, 103, 1));
    border-radius: 10rpx;
    position: absolute;
    transform: rotate(45deg);
    right: -57rpx;
    top: 29rpx;
    font-size: 20rpx;
    text-align: center;
    color: #fff;
}

.receive-btn {
    width: 150rpx;
    line-height: 48rpx;
    background: linear-gradient(90deg, rgba(85, 189, 255, 1), rgba(70, 153, 255, 1));
    border-radius: 24rpx;
    color: #fff;
    text-align: center;
    font-size: 28rpx;
    position: absolute;
    right: 20rpx;
    bottom: 20rpx;   
}

.coupon-condition {
    font-size: 24rpx;
    padding-top: 20rpx;
}

.price-amount-wrp {
    position: absolute;
    left: 50%;
    top: 50%;
    width: 100%;
    text-align: center;
    transform: translate(-50%, -50%);
}

.coupon-list-wrp { 
    background: #fff;
    position: fixed;
    width: 100%;    
    bottom: 0;
    left: 0;
}

.coupon-title {
    line-height: 100rpx;
    font-size: 32rpx;
    text-align: center;
    font-weight: 700;
}/*键盘*/
.index-type-select-nav li.active{font-size:14px;}

.icon-gray-solid-triangle,.keyboard-layer .cancel,
.keyboard-number .close i{
        background-image: url(https://j.vzan.cc/content/city/payonlineNew/img/tc-pic04.png);
        background-size: 1000rpx;
}

.ta-c { text-align: center; }
.keyboard-number{overflow: hidden}

.keyboard-number .li{
    text-align: center;
    line-height: 120rpx;
    height: 120rpx;
    font-size: 50rpx;
    width: 33.33%;
    float: left;
    border-right: 1px solid #f2f2f2;
    border-top: 1px solid #f2f2f2;
    box-sizing: border-box;
    cursor: pointer
}

.keyboard-layer .cancel{
    display: block;
    width: 80rpx;
    height: 44rpx;
    background-position: 0 -246rpx;
    margin: 0 auto;
}

.keyboard-layer .pay-bt{
    background:#ff3a1b;
    font-size: 40rpx;
    color: #fff;
    text-align: center;
    position: relative;
   opacity: .5;
}

.keyboard-layer .pay-bt.active{
    opacity: 1;
}

.keyboard-layer .cancel-bt{
    height:120rpx;
    padding-top:36rpx;
}

.keyboard-layer .f01{
    line-height:1.8;
    position: absolute;
    top:50%;
    left: 0;
    transform: translate(0,-50%);
    width: 100%;
}

.keyboard-number .close i{
    width: 32rpx;
    height: 20rpx;
    display: block;
    background-position:-588rpx -22rpx; 
    margin: 52rpx auto 0;
}

.keyboard-layer .dot i{
    width:10rpx;
    height:10rpx;
    border-radius: 50%;
    background: #333;
    margin:56rpx auto 0;
    display: block;
}


