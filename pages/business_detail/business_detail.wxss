.goods-car-layer {
  display: none;
  position: fixed;
  top: 0;
  left: 0;
  width: 100vw;
  height: 100vh;
  overflow: hidden;
  background: rgba(0, 0, 0, .36);
  z-index: 999
}

.close {
  position: absolute;
  top: 20rpx;
  right: 20rpx;
  width: 20px;
  height: 20px;
  border: 1rpx solid #ccc;
  border-radius: 50%;
  z-index: 2
}

.close::before, .close::after {
  background: #ccc
}

.goods-car-wrapper {
  position: absolute;
  bottom: -70vh;
  width: 100vw;
  height: 70vh;
  max-height: 70vh;
  padding: 0 20rpx;
  box-sizing: border-box;
  background: #fff;
  border-top-left-radius: 16rpx;
  border-top-right-radius: 16rpx;
  -webkit-backface-visibility: hidden;
  backface-visibility: hidden;
  -webkit-transform: translateZ(0);
  transform: translateZ(0)
}

@-webkit-keyframes slideFromDown {
  to {
    transform: translate3d(0, -100%, 0)
  }
}

@keyframes slideFromDown {
  to {
    transform: translate3d(0, -100%, 0);
    perspective: 1000
  }
}

.slideFromDown {
  -webkit-animation: slideFromDown .38s cubic-bezier(0.420, 0.010, 0.245, 0.775) forwards;
  animation: slideFromDown .38s cubic-bezier(0.420, 0.010, 0.245, 0.775) forwards
}

@-webkit-keyframes slideOutDown {
  from {
    transform: translate3d(0, -100%, 0)
  }
  to {
    transform: translate3d(0, 0, 0)
  }
}

@keyframes slideOutDown {
  from {
    transform: translate3d(0, -100%, 0)
  }
  to {
    transform: translate3d(0, 0, 0)
  }
}

.slideOutDown {
  -webkit-animation: slideOutDown .38s cubic-bezier(0.205, 0.680, 0.665, 1.000) forwards;
  animation: slideOutDown .38s cubic-bezier(0.205, 0.680, 0.665, 1.000) forwards
}

