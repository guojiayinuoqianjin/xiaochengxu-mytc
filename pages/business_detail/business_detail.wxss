@keyframes kf-marque-animation {
  0% {
    transform: translateX(0);
  }
  100% {
    transform: translateX(-90%);
  }
}

.marquee {
  width: 100%;
  height: 44px;
  line-height: 44px;
  border: none;
  display: block;
  margin: 0 auto;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: clip;
  position: relative;
  font-size: 14px;
}

.marquee .content {
  display: inline-block;
  position: relative;
  padding-right: 0px;
  padding-left: 20px;
  animation: kf-marque-animation 25s linear infinite;
  white-space: nowrap;
}

