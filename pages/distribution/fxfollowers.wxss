.top-line, .bottom-line, .right-line {
    position: relative;
}.flex {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    -ms-flex: 1;
    flex: 1;
}.c-f5a0 {
    color: #FF5A00;
}.f-14rem {
    font-size: 14px;
}.manage-cell {
    line-height: 1.2;
    padding-left:10px;
    background: #fff;
    margin-bottom: 5px;
}.manage-cell .top {
    padding: 10px 10px 10px 0;
    background: #fff;
}.bottom-line:after {
    content: " ";
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 1px;
    border-bottom: 1px solid #f0f0f0;
    -webkit-transform-origin: 0 100%;
    transform-origin: 0 100%;
    -webkit-transform: scaleY(0.5);
    transform: scaleY(0.5);
}.ta-r {
    text-align: right;
}.d-flex {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
}.manage-cell .top .head {
    width: 40px;
    height: 40px;
    margin-right: 10px;
    display: block;
}.f-12rem {
    font-size: 12px;
}.c-09cf {
    color: #009cff;
}