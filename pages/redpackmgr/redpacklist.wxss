.redItem .btn {
    position: relative;
    width: 76px;
    font-size: 12px;
    line-height: 1.8333333333;
    box-sizing: border-box;
    text-align: center
}

.redItem .btn::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 200%;
    height: 200%;
    border: solid 1px #ff5a00;
    border-radius: 6px;
    transform: scale(.5);
    transform-origin: 0 0
}

.redItem .btn:not(:last-of-type) {
    margin-right: 10px
}

.redNull {
    width: 198px;
    margin: auto;
    font-size: 12px
}

.redNull::before {
    display: block;
    content: '';
    width: 100%;
    height: 166px;
    background: url(../images/tc-yh-12.png) no-repeat;
    background-size: 100%
}

.redNullInfo {
    margin-top: 50px;
