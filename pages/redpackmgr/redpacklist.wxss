    color: #009cff
}

.sendRedLayer {
    position: fixed;
    top: 0;
    max-width: 640px;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, .42)
}

.layerContent {
    margin: 47.76% auto 0;
    width: 80%;
    padding: 9.95% 12px 12.94%;
    background: #fff;
    text-align: center;
    border-radius: 6px;
}

.layerContent .title {
    font-weight: 400;
    font-size:13px;
}

.layerContent .btn {
    margin-top: 8.28%;
    padding: 4.96% 0;
    font-size: 15px;
    color: #fff;
    background: linear-gradient(to right, #fd861d, #ed4d0d);
    border-radius: 8px
}
