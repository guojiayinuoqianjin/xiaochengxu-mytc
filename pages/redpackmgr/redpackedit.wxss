html,body{
    width: 100%;
    height: 100%
}
body{
    position: relative;
    max-width: 640px;
    margin: 0 auto;
    font-family: PingFang-SC;
    line-height: 1;
    font-weight: 500
}
header{
    text-align: center;
    font-size: 16px;
    line-height: 2.8125;
    color: #fff;
    background: linear-gradient(to right,#fe3d49,#ed4d0d)
}
.sendPocketList li{
    position: relative;
    padding: 14px 1rem;
    font-size: 14px;
    background: #fff
}
.sendPocketList li::after{
    width: calc(100% - 2rem);
    right: 0;
    margin: 0 auto
}
.shareItem:active{
    background: rgba(255, 255, 255,.12)
}
input[type="number"]{
    line-height: 1;
    font-size: 14px
}
.tagName{
    width: 22.268%;
    font-size: 14px
}
.locationIcon::before{
    content: '';
    display: inline-block;
    width:8px;
    height:1rem;
    margin-right:8px;
    vertical-align: middle;
    /* background:url() */
    background: #ff5a00
}
footer{
    text-align: center;
    line-height: 3.57142857;
    font-size: 14px;
    background: #fff
}
.tips{
    padding-top:14px;
    padding-right: 1rem;
    padding-left: 1rem;
    font-size: 12px;
    background: #fff
}
.limitLocation,.tips:last-of-type{
    margin-top: 5px
}
.tips:first-of-type{
    padding-bottom: .6rem
}
.tips:last-of-type{
    padding-bottom: 14px;
}
.payNum{
    color: #ff5a00
}
.payment{
    color: #fff;
    background:#ff5a00
}

/* 分享的页面 */
@keyframes slideFromDown{
    from{
        transform: translateY(100%)
    }
    to{
        transform:none
    }
}
.slideFromDown{
    animation: slideFromDown .48s cubic-bezier(0.125, 0.300, 0.255, 1.000) both
}
@keyframes slideOutDown{
    to{
        transform: translateY(100%)
    }
}
.slideOutDown{
    animation: slideOutDown .92s cubic-bezier(0.610, 0.100, 0.695, 0.660) both
}
#shareLayer {
    position: fixed;
    top: 0;
    right: 0;
    left: 0;
    width: 100%;
    height: 100%;
    margin: 0 auto;
    max-width: 640px;
    background: rgba(0, 0, 0, .42)
}

.shareLayerContent {
    padding-bottom:8px;
    background: #f0f0f0
}

.shareTitle {
    font-weight: 500;
    font-size:16px;
    line-height: 2.8125;
    text-align: center;
    color: #fff;
    background: #fe3d49
}
.shareList{
    margin-bottom: 6px;
    overflow-y: scroll;
    -webkit-overflow-scrolling: touch
}
.shareItem {
    position: relative;
    background: #fff;
    transition: all 3.8s
}
.shareItem:not(:first-of-type){
    margin-top: 5px
}
.shareItem a{
    display: block
}
.shareItem a,.shareItemList li{
    position: relative;
    padding: 12px 15px;
    font-size: 14px;
}
.shareItem a .arrow{
    position: absolute;
    right:15px;
    top:0;
    bottom: 0;
    width: 16px;
    height: 12px;
    margin: auto 0;
    color: #999;
    -webkit-transition: all 0.4s ease;
	transition: all 0.4s ease
}
.shareItem.open .arrow{
    -webkit-transform: rotate(180deg);
	transform: rotate(180deg)
}

.shareItemList li:not(:last-of-type)::after {
    content: " ";
    position: absolute;
    left: 0;
    right: 0;
    margin: 0 auto;
    bottom: 0;
    width: calc(100% - 30px);
    height: 1px;
    border-bottom: 1px solid #f0f0f0;
    -webkit-transform-origin: 0 100%;
    transform-origin: 0 100%;
    -webkit-transform: scaleY(0.5);
    transform: scaleY(0.5)
}
.shareItemList li.active{
    color: #fff;
    background: #fe3d49
}
.shareItemList li:active {
    background: rgba(0, 0, 0, .08)
}

.shareBtn{
    width: 87.2%;
    margin:auto auto 0;
    color: #fff;
    line-height: 2.5;
    font-size: 16px;
    text-align: center;
    border-radius: 8px;
    background: #fe3d49
}
#shareLayer .close-layer{
    margin-top: 4.975%;
    margin-bottom: 2px;
}.iconfont {
    font-family: "iconfont";
    font-size: 16px;
    font-style: normal;
}.menuItem{
    position: relative;
    padding:12px 15px;
    font-size: 14px;
}.shareItemList .li.active {
    color: #fff;
   background: #fe3d49
}.shareItemList .li {
    position: relative;
    padding: 12px 15px;
    font-size: 14px;
}.bottom-line:after {
    content: " ";
    position: absolute;
