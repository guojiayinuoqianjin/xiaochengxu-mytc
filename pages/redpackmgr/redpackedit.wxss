.write-infor-cell.plr_10 { padding-left: 10px; padding-right: 10px; }

.write-infor-cell:after { content: " "; position: absolute; left: 15px; bottom: 0; width: 100%; height: 1px; border-bottom: 1px solid #f0f0f0; -webkit-transform-origin: 0 100%; transform-origin: 0 100%; -webkit-transform: scaleY(0.5); transform: scaleY(0.5); }

.write-infor-cells .write-infor-cell:last-child:after { display: none; }

.write-infor-cell .write { color: #666; text-align: right; padding-left: 15px; position: relative; }

.write-infor-cell .write .light-grey { color: #999; }

.write-infor-cell .write-title { line-height: 25px; }

.write-infor-cell .shuru-input { box-sizing: border-box; width: 100%; text-align: right; height: 25px; display: block; color: #666; font-size: 15px; line-height: 25px; }

.chengtuan-time-input { width: 25px; box-sizing: border-box; text-align: left; height: 25px; display: inline-block; color: #666; font-size: 15px; line-height: 25px; }

.write-infor-cell .shuru-input::-webkit-input-placeholder { color: #999; }

.write-infor-cell .shuru-input::-moz-placeholder { color: #999; }

.write-infor-cell .shuru-input::-moz-placeholder { color: #999; }

.write-infor-cell .shuru-input::-ms-input-placeholder { color: #999; }

.write-infor-cell .bt { box-sizing: border-box; margin-left: 10px; padding-left: 10px; position: relative; height: 25px; }

.write-infor-cell .bt .code-input { margin-top: .10px; color: #ff5a00; font-size: 13px; border: 1px solid #ff5a00; border-radius: 2px; box-sizing: border-box; padding: 3px; display: block; min-width: 45px; }

.write-infor-cell .bt:after { content: " "; position: absolute; left: 0; top: 0; height: 100%; width: 1px; bottom: 0; border-left: 1px solid #f0f0f0; color: #f0f0f0; -webkit-transform-origin: 0 0; transform-origin: 0 0; -webkit-transform: scaleX(0.5); transform: scaleX(0.5); }

.write-infor-cell .write.arrow { padding-right: 10px; }

.write-infor-cell .write.arrow:after { content: " "; display: inline-block; height: 6px; width: 6px; border-width: 1px 1px 0 0; border-color: #666; border-style: solid; position: relative; position: absolute; top: 50%; right: 0; transform: translate(0, -50%) rotate(45deg); -webkit-transform: translate(0, -50%) rotate(45deg); margin-right: 3px; }

.write-infor-cell .shuru-select { box-sizing: border-box; width: 100%; text-align: right; height: 25px; display: block; color: #666; font-size: 15px; -webkit-appearance: none; appearance: none; background: transparent; border: none; outline: none; overflow: hidden; text-overflow: ellipsis; white-space: nowrap; -webkit-tap-highlight-color: transparent; direction: rtl; }

.write-infor-cell .shuru-select option { color: #999; }

.write-infor-cell .bt .location-bt { margin-top: 3px; color: #ff5a00; display: block; }

.write-infor-cell .write .shop-head { width: 45px; height: 45px; display: block; border-radius: 50%; overflow: hidden; }

.write-infor-cell .write .shop-head img { width: 100%; height: 100%; }

.write-infor-cell .p01.orange-f { color: #ff5a00; }

