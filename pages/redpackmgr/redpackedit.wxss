.write-infor-cells { margin-top: 10px; overflow: hidden; background: #fff; }

.write-infor-cell { position: relative; padding: 10px 15px; color: #333; font-size: 15px; line-height: 1.25; }

.write-infor-cell .width_02 { width: 35px; }
.write-infor-cell .width_03 { width: 100px; }
