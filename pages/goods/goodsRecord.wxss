view {
    line-height: 1;
}
#grid{
    background: #fff
}
.gridItem{
    padding:40rpx 0;
    box-sizing: border-box;
    text-align: center;
    width: 50%;
    border-bottom: 1rpx solid #f2f2f2
}
.gridItem:nth-child(2n+1){
    border-right: 1rpx solid #f2f2f2
}
.gridItem .title{
    font-size: 32rpx;
    margin-bottom: 16rpx
}
.title::before{
    display: inline-block;
    content: '';
    width: 30rpx;
    height:30rpx;
    vertical-align: middle;
    margin-right: 8rpx
}
.gridItem:nth-child(1) .title::before{
    background: url(data:image/png;base64,);
    background-size: 100%
}
.gridItem:nth-child(2) .title::before{
    background: url(data:image/png;base64,);
    background-size: 100%
}
.gridItem:nth-child(3) .title::before{
    background: url(data:image/png;base64,);
    background-size: 100%
}
.gridItem:nth-child(4) .title::before{
    background: url(data:image/png;base64,);
    background-size: 100%
}
.gridItem text{
    margin-top:4rpx;
    font-size: 30rpx
}
.tips{
    margin-bottom: 20rpx;
    padding: 20rpx;
    font-size: 24rpx;
    background: #fff
}
.item{
    margin-bottom: 20rpx;
    background: #fff;
}
.item-top{
    padding: 20rpx;
    border-bottom: 1rpx solid #f2f2f2
}
.avatar{
    width:150rpx;
    height:150rpx;
    margin-right: 20rpx;
    border-radius: 50%;
    overflow: hidden
}
.avatar image{
    width: 100%;
    height: 100%
}
.customer{
    font-size: 30rpx;
    color: #333
}
.phone{
    font-size: 28rpx;
    color: #333
}
.date{
    font-size: 28rpx;
    color: #999
}
.contact{
    width: 140rpx;
    padding: 12rpx;
    text-align: center;
    font-size: 28rpx;
    border-radius: 8rpx;
    color: #009cff;
    border: 1rpx solid #009cff
}
.item-bot{
    padding: 20rpx;
    font-size: 28rpx
}
.price{
    font-size:32rpx;
    color:#fe3d49
}
.price::before{
    content: "ï¿¥";
    font-size: 24rpx;
    vertical-align: 1px;
    color:#fe3d49
}
.status{
    color: #009cff
}
.loadingAll{
    width:100%;
    text-align:center;
    padding:20rpx 0;
    font-size:22rpx;
    color:#aaa;
    background-color:#fff
}