.tc-list-top-filter-f {
    overflow: auto;
    line-height: normal;
    height: auto;
    width: 100%;
    display: inline-block;
}
.tc-list-top-filter {
    width: 100%;
}
.tc-list-top-filter {
    height: 42px;
    color: #000;
    text-align: center;
    line-height: 35px;
    background: #fff;
    position: relative;
    z-index: 99;
    display: flex;
    width: 100%;
}
.tc-list-top-filter .averagebox .active {
    color: #ff6356;
}.tc-list-top-filter .tinybox {
    display: block;
    height: 100%;
    width: 100%;
    color: #000;
    font-size: 14px;
}
.tc-list-top-filter .averagebox {
    width: 25%;
    float: left;
    position: relative;
    flex: 1;
}
.tc-list-top-filter .averagebox :after {
    content: " ";
    position: absolute;
    left: 0;
    top: 11px;
    width: 1px;
    height: 12px;
    border-left: 1px solid #ddd;
    color: #ddd;
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    -webkit-transform: scaleX(0.5);
    transform: scaleX(0.5);
}
.active .tc-list-top-triangle {
    content: "";
    width: 10px;
    height: 5px;
    vertical-align: 2px;
    margin-left: 5px;
    display: inline-block;
    background: url(https://j.vzan.cc/content/city/images/tongcheng-01.png?1206) no-repeat -152.9px -37.9px;
    background-size: 500px;
    vertical-align: 4px;
    overflow: hidden;
}
.tc-list-top-triangle {
    width: 10px;
